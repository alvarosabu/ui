{
  "name": "@alvarosabu/ui",
  "type": "module",
  "version": "3.0.0-alpha.7",
  "packageManager": "pnpm@8.3.1",
  "description": "UI Library for my personal projects",
  "author": "Alvaro Saburido <hola@alvarosaburido.dev> (https://github.com/alvarosabu/)",
  "license": "MIT",
  "keywords": [
    "ui",
    "vue",
    "composable"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/as-ui.d.ts",
      "import": "./dist/as-ui.js"
    },
    "./styles": "./dist/styles.css",
    "./*": "./*"
  },
  "main": "./dist/as-ui.js",
  "module": "./dist/as-ui.js",
  "types": "./dist/as-ui.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.lib.config.ts",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx .",
    "test": "vitest",
    "release": "release-it",
    "release:alpha": "release-it --preRelease=alpha",
    "test:ui": "vitest --ui",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview"
  },
  "dependencies": {
    "@vueuse/core": "^10.2.1",
    "sass": "^1.64.1",
    "shikey": "^0.0.2",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@alvarosabu/eslint-config": "^0.4.0",
    "@alvarosabu/eslint-config-vue": "^0.4.0",
    "@histoire/plugin-vue": "^0.16.1",
    "@iconify-json/ant-design": "^1.1.6",
    "@iconify-json/carbon": "^1.1.18",
    "@iconify-json/gg": "^1.1.5",
    "@iconify-json/logos": "^1.1.33",
    "@iconify-json/octicon": "^1.1.44",
    "@iconify-json/teenyicons": "^1.1.5",
    "@iconify/json": "^2.2.95",
    "@release-it/conventional-changelog": "^7.0.0",
    "@unocss/preset-web-fonts": "^0.53.6",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/ui": "^0.33.0",
    "eslint": "^8.45.0",
    "histoire": "^0.16.2",
    "kolorist": "^1.8.0",
    "pathe": "^1.1.1",
    "release-it": "^16.1.3",
    "typescript": "^5.1.6",
    "unocss": "^0.53.6",
    "vite": "^4.4.7",
    "vite-plugin-banner": "^0.7.0",
    "vite-plugin-dts": "^3.3.1",
    "vite-svg-loader": "^4.0.0",
    "vitest": "^0.33.0"
  }
}
